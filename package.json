{
  "name": "react-stores",
  "version": "2.11.0",
  "keywords": [
    "react",
    "stores",
    "reactive",
    "flux",
    "states",
    "components"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "React direct state stores",
  "main": "lib",
  "scripts": {
    "build": "tsc src/store.ts --jsx 'react' --target es5 --lib es6,dom -d --outDir './' && mkdir -p ./lib && mv ./store.d.ts ./index.d.ts && mv ./store.js ./lib/index.js",
    "test": "yarn jest",
    "demo": "webpack-dev-server --progress --config ./demo/webpack.config.js --inline",
    "release:demo": "webpack --config ./demo/webpack.prod.config.js",
    "prepare": "yarn build && yarn test"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "tests/.*[.](tsx?)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "IBIT LTD.",
  "license": "MIT",
  "dependencies": {
    "freezer-js": "^0.14.1",
    "react-testing-library": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "awesome-typescript-loader": "^3.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "expect": "^1.20.2",
    "expect-jsx": "^3.0.0",
    "html-loader": "^0.4.5",
    "jest": "^23.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hooks-testing-library": "^0.3.6",
    "react-tools": "^0.13.3",
    "should": "^11.2.0",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^23.1.3",
    "typescript": "^3.1.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "repository": "https://github.com/ibitcy/react-store.git",
  "peerDependencies": {
    "react": "^16.8.4"
  }
}
