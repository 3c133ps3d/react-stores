{
  "name": "react-stores",
  "version": "2.11.0",
  "keywords": [
    "react",
    "stores",
    "reactive",
    "flux",
    "states",
    "components"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "React stores",
  "main": "lib",
  "scripts": {
    "build": "tsc src/store.ts --jsx 'react' --target es5 --lib es6,dom -d --outDir './' && mkdir -p ./lib && mv ./store.d.ts ./index.d.ts && mv ./store.js ./lib/index.js",
    "test": "npx jest",
    "demo": "webpack-dev-server --progress --config ./demo/webpack.config.js --inline",
    "release:demo": "webpack --config ./demo/webpack.prod.config.js",
    "prepare": "npm run build && npm run test"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "tests/.*[.](tsx?)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "IBIT LTD.",
  "license": "MIT",
  "dependencies": {
    "freezer-js": "^0.14.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "acorn": "^6.1.1",
    "copy-webpack-plugin": "^5.0.2",
    "expect": "^24.7.1",
    "expect-jsx": "^5.0.0",
    "html-loader": "^0.5.5",
    "jest": "^24.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-testing-library": "^0.4.0",
    "react-testing-library": "^6.0.4",
    "react-tools": "^0.13.3",
    "should": "^13.2.3",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "repository": "https://github.com/ibitcy/react-store.git",
  "peerDependencies": {
    "react": "^16.8.4"
  }
}
