{
  "name": "react-stores",
  "version": "2.12.1",
  "keywords": [
    "react",
    "stores",
    "reactive",
    "flux",
    "states",
    "components"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "React stores",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && rollup --config ./rollup.config.js && npm run check-size --scripts-prepend-node-path",
    "check-size": "size-limit",
    "test": "npx jest",
    "demo": "webpack-dev-server --progress --config ./demo/webpack.config.js --inline",
    "release:demo": "webpack --config ./demo/webpack.prod.config.js",
    "prepare": "npm run build && npm run check-size && npm run test"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "3kb",
      "webpack": false
    }
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "tests/.*[.](tsx?)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "IBIT LTD.",
  "license": "MIT",
  "dependencies": {
    "freezer-js": "^0.14.1"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@testing-library/react-hooks": "^1.0.4",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "acorn": "^6.1.1",
    "copy-webpack-plugin": "^5.0.3",
    "expect": "^24.8.0",
    "expect-jsx": "^5.0.0",
    "html-loader": "^0.5.5",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "react-tools": "^0.13.3",
    "rollup": "^1.16.2",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "should": "^13.2.3",
    "size-limit": "^1.3.7",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": "https://github.com/ibitcy/react-store.git",
  "peerDependencies": {
    "react": "^16.8.6"
  }
}
