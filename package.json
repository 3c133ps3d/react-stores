{
  "name": "react-stores",
  "version": "2.12.2",
  "keywords": [
    "react",
    "stores",
    "reactive",
    "flux",
    "states",
    "redux",
    "state",
    "components"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "React stores",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && rollup --config ./rollup.config.js && npm run check-size --scripts-prepend-node-path",
    "check-size": "size-limit",
    "pretest": "npm run build",
    "test": "npx jest",
    "demo": "webpack-dev-server --progress --config ./demo/webpack.config.js --inline",
    "release:demo": "webpack --config ./demo/webpack.prod.config.js",
    "prepare": "npm run build && npm run check-size && npm run test"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "3kb",
      "webpack": false
    }
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "tests/.*[.](tsx?)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "IBIT LTD.",
  "license": "MIT",
  "dependencies": {
    "freezer-js": "^0.14.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.6",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.0.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "acorn": "^7.1.0",
    "copy-webpack-plugin": "^5.0.4",
    "expect": "^24.9.0",
    "expect-jsx": "^5.0.0",
    "html-loader": "^0.5.5",
    "jest": "^24.9.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "react-tools": "^0.13.3",
    "rollup": "^1.26.3",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.25.2",
    "should": "^13.2.3",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "repository": "https://github.com/ibitcy/react-store.git",
  "peerDependencies": {
    "react": "^16.8.6"
  }
}
